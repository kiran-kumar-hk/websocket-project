if selected_branch and switch_branch(repo_dir, selected_branch):
        csv_files = []
        for root, dirs, files in os.walk(repo_dir):
            for file in files:
                if file.endswith('.csv'):
                    csv_files.append(os.path.relpath(os.path.join(root, file), repo_dir))
